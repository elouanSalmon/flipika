{
  "name": "ads",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "echo '❌ ERROR: Please use \"npm run build:dev\" or \"npm run build:prod\"' && exit 1",
    "build:dev": "tsc -b && vite build --mode development && node scripts/generate-sitemap.js",
    "build:prod": "tsc -b && vite build --mode production && node scripts/generate-sitemap.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "echo '❌ ERROR: Please use \"npm run deploy:dev\" or \"npm run deploy:prod\"' && exit 1",
    "deploy:dev": "npm run build:dev && firebase deploy -P dev",
    "deploy:prod": "npm run build:prod && firebase deploy -P production",
    "deploy:hosting:dev": "npm run build:dev && firebase deploy --only hosting -P dev",
    "deploy:hosting:prod": "npm run build:prod && firebase deploy --only hosting -P production",
    "deploy:functions:dev": "firebase deploy --only functions -P dev",
    "deploy:functions:prod": "firebase deploy --only functions -P production",
    "firebase:init": "firebase init",
    "firebase:login": "firebase login"
  },
